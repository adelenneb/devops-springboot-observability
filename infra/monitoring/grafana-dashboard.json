{
  "id": null,
  "title": "Fifth App Overview",
  "uid": "fifth-app",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "tags": ["spring-boot", "prometheus"],
  "panels": [
    {
      "type": "timeseries",
      "title": "Heap Memory Used",
      "targets": [
        {
          "expr": "sum by (area) (jvm_memory_used_bytes{application=\"fifth-app\",area=\"heap\"})",
          "legendFormat": "{{area}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "Process CPU Usage",
      "targets": [
        {
          "expr": "process_cpu_usage{application=\"fifth-app\"}",
          "legendFormat": "cpu"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "max": 1
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "HTTP Requests (rps)",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{application=\"fifth-app\"}[5m])) or vector(0)",
          "legendFormat": "requests/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 }
    }
  ]
}
